@model Cv.Models.Entity.TblSkill
@{
    ViewBag.Title = "UpdateSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3 class="mt-3">Yetenek Güncelle</h3>

<form method="post" class="form-group">
    @Html.Label("ID")
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control mb-3 ", style = "display: none;" })

    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })

    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x => x.Rate, new { @class = "form-control" })

    <button class="btn btn-outline-success mt-3">Güncelle</button>
</form>
